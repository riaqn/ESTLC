Type list_int = 
| nil : list_int
| cons_int : int -> list_int -> list_int

Func main (l : list_int) : list_int = 
match l
| nil => l
| cons_int x l' => match l'
                   | nil  => l
                   | cons_int x' l'' => match (< x x')
                                        | false => l
                                        | true => l'
